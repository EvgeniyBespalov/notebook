<%= form_for :user do |f|%>
<table class = "tableUser">
  <tr>
    <td>
      <%= t('user') %>
    </td>
  </tr>
  <tr>
    <td>
      <%= f.text_field :login%>
    </td>
  </tr>
  <tr>
    <td>
      <%= t('password') %>
    </td>
  </tr>
  <tr>
    <td>
      <%= f.password_field :password%>
    </td>
  </tr>
  <tr>
    <td>
      <%= f.button t('login')%>
    </td>
  </tr>	
</table>
<% end %>